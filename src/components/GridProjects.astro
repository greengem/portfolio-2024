---
interface Props {
	title: string;

}

const { title } = Astro.props;
---
<div class="mb-10 last:mb-0">
    <p class="text-xl mb-5">{title}</p>
    <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 xl:grid-cols-4 2xl:grid-cols-5 3xl:grid-cols-6 gap-5">
        <slot />
    </div>
</div>
